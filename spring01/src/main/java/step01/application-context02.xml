<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
        
  <!-- id가 없으면 name 속성값을 아이디로 사용한다. -->      
  <bean name="car1" class="step01.Car"/>
  
  <!-- id와 name이 모두 있으면, name은 별명으로 사용한다. -->
  <bean id="car2" name="car3" class="step01.Car"/>
  
  <!-- id는 이름을 한 개만 지정할 수 있지만, name은 여러 개의 이름을 지정할 수 잇다. 
  첫 번째 이름은 아이디로 사용, 나머지는 별명으로 사용
  객체를 찾을 때는 아이디나 별명으로 구분하지 않는다. "," ";" " " 으로 구분 가능 -->
  <bean name="car4,car5,car6" class="step01.Car"/>
  <bean name="car7;car8;car9" class="step01.Car"/>
  <bean name="car10 car11 car12" class="step01.Car"/>
  
  
  
</beans>





